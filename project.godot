config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Mob",
"language": "GDScript",
"path": "res://Scripts/Mob.gd"
}, {
"base": "Mob",
"class": "Player",
"language": "GDScript",
"path": "res://Scripts/Player.gd"
} ]
_global_script_class_icons={
"Mob": "",
"Player": ""
}

[application]

config/name="Inherit-test"
run/main_scene="res://Scenes/GameWorld.tscn"
